{
    "name": "galadrieljs",
    "version": "1.0.0-beta.11",
    "description": "A versatile JavaScript library empowering developers to create elegant, dynamically responsive user interfaces. Focused on modularity and simplicity, enabling the crafting of captivating web applications with optimized performance. Unleash your creativity and let Galadriel.js illuminate your design process.",
    "main": "./build/main.js",
    "bin": {
        "galadriel-init": "./build/init-scripts/initConfig.js"
    },
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "dev": "npx webpack serve --config webpack.config.js",
        "start": "npx webpack --watch",
        "prebuild": "rm -rf build && tsc --outDir build && node build/plugins/TypeWeaver/typeForgePlugin.js",
        "build": "webpack --mode production"
    },
    "type": "commonjs",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/patrickgunnar/galadriel.js.git"
    },
    "keywords": [
        "javascript",
        "library",
        "web",
        "frontend",
        "UI",
        "user-interface",
        "styling",
        "modularity",
        "performance",
        "responsive",
        "elegant",
        "dynamic"
    ],
    "author": "Patrick Gunnar",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/patrickgunnar/galadriel.js/issues"
    },
    "homepage": "https://github.com/patrickgunnar/galadriel.js#readme",
    "devDependencies": {
        "@babel/cli": "^7.23.0",
        "@babel/core": "^7.23.0",
        "@types/jest": "^29.5.5",
        "@types/lodash": "^4.14.199",
        "autoprefixer": "^10.4.15",
        "babel-loader": "^9.1.3",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "cssnano": "^6.0.1",
        "jest": "^29.7.0",
        "mini-css-extract-plugin": "^2.7.6",
        "postcss": "^8.4.30",
        "postcss-loader": "^7.3.3",
        "postcss-preset-env": "^9.1.4",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.4.4",
        "typescript": "^5.2.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "yarn": "^1.22.19"
    },
    "directories": {
        "test": "test"
    },
    "peerDependencies": {
        "@babel/cli": "^7.23.0",
        "@babel/core": "^7.23.0",
        "autoprefixer": "^10.4.15",
        "babel-loader": "^9.1.3",
        "css-loader": "^6.8.1",
        "cssnano": "^6.0.1",
        "mini-css-extract-plugin": "^2.7.6",
        "postcss": "^8.4.30",
        "postcss-loader": "^7.3.3",
        "postcss-preset-env": "^9.1.4",
        "ts-loader": "^9.4.4"
    },
    "dependencies": {
        "@babel/generator": "^7.23.0",
        "@babel/preset-env": "^7.22.20",
        "@babel/preset-typescript": "^7.23.0",
        "lodash": "^4.17.21"
    }
}
